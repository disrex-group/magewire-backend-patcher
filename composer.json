{
    "name": "disrex/magewire-backend-patcher",
    "type": "composer-plugin",
    "description": "Applies Magewire backend patch automatically when used with Magewire",
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "composer-plugin-api": "^2.0"
    },
    "authors": [
        {
            "name": "Rick Bouma",
            "email": "r.bouma@disrex.nl",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Composer\\MagewirePatchPlugin\\": "Composer/MagewirePatchPlugin/"
        }
    },
    "extra": {
        "class": "Composer\\MagewirePatchPlugin\\Plugin"
    }
}
